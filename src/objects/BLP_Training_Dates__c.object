        <type>Text</type>
    </fields>
    <fields>
        <fullName>BLP_Course_Location__c</fullName>
        <label>Course Location</label>
        <length>255</length>
        <trackHistory>true</trackHistory>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>BLP_Course_Room__c</fullName>
        <label>Course Room</label>
        <length>255</length>
        <trackHistory>true</trackHistory>
        <type>Text</type>
    </fields>
