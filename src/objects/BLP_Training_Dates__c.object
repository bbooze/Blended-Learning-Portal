    <fields>
        <fullName>BLP_Reference_Person_Email__c</fullName>
        <label>Reference Person Email</label>
        <trackHistory>true</trackHistory>
        <type>Email</type>
    </fields>
    <fields>
        <fullName>BLP_Reference_Person__c</fullName>
        <label>Reference Person</label>
        <length>255</length>
        <trackHistory>true</trackHistory>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>BLP_Tech_Support_Email__c</fullName>
        <description>Tech Support Email will be filled from Course-&gt; Tech Support Email by Process Builder BLP Course Summary Notification (Training Date)</description>
        <label>Tech Support Email</label>
        <type>Email</type>
    </fields>
    <fields>
        <fullName>BLP_Tech_Support__c</fullName>
        <formula>BLP_Course__r.BLP_Tech_Support__c</formula>
