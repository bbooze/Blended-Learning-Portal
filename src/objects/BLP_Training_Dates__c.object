        <lookupDialogsAdditionalFields>BLP_Course_Location__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>BLP_Course__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>BLP_Course_Expert__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>BLP_Tech_Support__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>BLP_Max_Capacity__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>BLP_Number_of_Attendees_Registered__c</lookupDialogsAdditionalFields>
    </searchLayouts>
