        <fullName>BLP_Review_Reminder_Date__c</fullName>
        <formula>IF(
  ISPICKVAL(BLP_Review_Reminder_Notifications_Months__c, &quot;12&quot;),
  (DATE((YEAR(BLP_Review_Date__c) - 1),MONTH(BLP_Review_Date__c),DAY(BLP_Review_Date__c))),
  IF(ISPICKVAL(BLP_Review_Reminder_Notifications_Months__c, &quot;6&quot;),BLP_Review_Date__c - (6 * 30 + 2), IF(ISPICKVAL(BLP_Review_Reminder_Notifications_Months__c, &quot;3&quot;),BLP_Review_Date__c - (3 * 30 + 1),BLP_Review_Date__c)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Review Reminder Date</label>
        <type>Date</type>
